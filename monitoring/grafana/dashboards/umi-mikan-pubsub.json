{
  "id": null,
  "title": "umi.mikan Pub/Sub Monitoring",
  "tags": ["umi-mikan", "pubsub", "scheduler", "subscriber"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Scheduler Job Execution Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(scheduler_job_executions_total[5m])",
          "legendFormat": "{{job_name}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Scheduler Job Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(scheduler_job_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile - {{job_name}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(scheduler_job_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile - {{job_name}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Users with Auto Summary Enabled",
      "type": "stat",
      "targets": [
        {
          "expr": "scheduler_users_with_auto_summary",
          "legendFormat": "{{summary_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Messages Queued to Redis",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(scheduler_queued_messages_total[5m])",
          "legendFormat": "{{message_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Subscriber Message Processing Count",
      "type": "timeseries",
      "targets": [
        {
          "expr": "subscriber_messages_processed_total",
          "legendFormat": "{{message_type}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      }
    },
    {
      "id": 6,
      "title": "Subscriber Processing Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(subscriber_processing_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile - {{message_type}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(subscriber_processing_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile - {{message_type}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      }
    },
    {
      "id": 7,
      "title": "Summaries Generated (Last Hour)",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(subscriber_summaries_generated_total[1h])",
          "legendFormat": "{{summary_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 20
      }
    },
    {
      "id": 8,
      "title": "Job Execution Success Rate (%)",
      "type": "stat",
      "targets": [
        {
          "expr": "100 * rate(scheduler_job_executions_total{status=\"success\"}[5m]) / rate(scheduler_job_executions_total[5m])",
          "legendFormat": "{{job_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Message Processing Success Rate (%)",
      "type": "stat",
      "targets": [
        {
          "expr": "100 * rate(subscriber_messages_processed_total{status=\"success\"}[5m]) / rate(subscriber_messages_processed_total[5m])",
          "legendFormat": "{{message_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
